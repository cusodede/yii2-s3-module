{
	"name": "cusodede/yii2-s3-module",
	"description": "S3 support module",
	"type": "yii2-module",
	"keywords": [
		"yii2",
		"module",
		"cloud storage",
		"minio",
		"S3"
	],
	"license": "proprietary",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		},
		{
			"name": "Ivan Suares",
			"email": "pepe_botika69@yahoo.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.0",
		"aws/aws-sdk-php": "^3.208",
		"pozitronik/yii2-traits": "dev-master",
		"pozitronik/yii2-badgewidget": "dev-master",
		"pozitronik/yii2-grid-config": "dev-master",
		"limion/yii2-jquery-fileupload-widget": "~1.0"
	},
	"require-dev": {
		"yiisoft/yii2": "dev-master",
		"yiisoft/yii2-bootstrap4": "^2.0.10"
	},
	"autoload": {
		"psr-4": {
			"cusodede\\s3\\": "src"
		}
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
